<% content_for :title do %>IMLS Muesum Dashboard<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="welcome" data-view="Text" data-title="Hello" data-text="This is the Raleigh dashboard powered by dashing.rb, Socrata, and open data!" data-moreinfo="Protip: You can drag the widgets around!"></div>
    </li>
    <li data-row="1" data-col="3" data-sizex="2" data-sizey="3">
      <div data-id="count_by_issue_type" data-view="List" data-unordered="false" data-title="Count of Tickets by Issue Type" data-moreinfo=""></div>
    </li>
    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="count_by_status" data-view="List" data-unordered="true" data-title="Count of Tickets by Status" data-moreinfo=""></div>
    </li>
    <li data-row="2" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="total_tickets" data-view="Number" data-title="Total # of Tickets" data-moreinfo="" data-prefix=""></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="number_with_photo" data-view="Number" data-title="Number Tickets with Photo" data-moreinfo="" data-prefix="" data-suffix=""></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="percent_with_photo" data-view="Meter" data-title="Percent of Tickets with Photo" data-min="0" data-max="100" data-suffix="%"></div>
    </li>
    <li data-row="4" data-col="2" data-sizex="2" data-sizey="2">
      <div data-id="pie_iframe" data-view="Iframe" data-url="https://brigades.opendatanetwork.com/w/je6i-p6w3/?cur=qmGoyygFkTK&from=root"></div>
    </li>
  </ul>
</div>