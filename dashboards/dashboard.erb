<% content_for :title do %>IMLS Muesum Dashboard<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="welcome" data-view="Text" data-title="Hello" data-text="This is a dashboard powered by data.imls.gov, dashing.rb, Socrata, and open data!" data-moreinfo="Built by @Skram during Open Data Day DC 2015. github.com/marks/museum-dash"></div>
    </li>

    <li data-row="1" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="total_museums" data-view="Number" data-title="Total # of Museums" data-moreinfo="" data-prefix=""></div>
    </li>

    <li data-row="2" data-col="2" data-sizex="2" data-sizey="1">
      <div data-id="count_by_type" data-view="List" data-unordered="false" data-title="Museums by Type" data-moreinfo=""></div>
    </li>

<!--     <li data-row="1" data-col="3" data-sizex="2" data-sizey="3">
      <div data-id="count_by_issue_type" data-view="List" data-unordered="false" data-title="Count of Tickets by Issue Type" data-moreinfo=""></div>
    </li>
    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="count_by_status" data-view="List" data-unordered="true" data-title="Count of Tickets by Status" data-moreinfo=""></div>
    </li> -->
    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="total_nonprofit" data-view="Number" data-title="Number Nonprofit" data-moreinfo="" data-prefix="" data-suffix=""></div>
    </li>
    <li data-row="2" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="percent_nonprofit" data-view="Meter" data-title="Percent Nonprofit" data-min="0" data-max="100" data-suffix="%"></div>
    </li>


    <li data-row="1" data-col="4" data-sizex="1" data-sizey="2">
      <div data-id="count_by_state" data-view="List" data-unordered="true" data-title="Top/Bottom States by Count" data-moreinfo=""></div>
    </li>

  </ul>
</div>